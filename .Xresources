Xft.dpi: 97
Xft.antialias: 1
Xft.hinting: 1
Xft.rgba: rgb
Xft.autohint: 0
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

*.scrollBar:                false
*.cursorBlink:              true
*.internalBorder:           25
*.keysym.C-Left:            \033[1;5D
*.keysym.C-Right:           \033[1;5C
*.keysym.C-Up:              \033[1;5A
*.keysym.C-Down:            \033[1;5B
*.keysym.M-Left:            \033[1;3D
*.keysym.M-Right:           \033[1;3C
*.keysym.M-C-k:             33[1;7B
*.keysym.M-C-j:             33[1;7C
*.keysym.Shift-Up:          command:\033]720;1\007
*.keysym.Shift-Down:        command:\033]721;1\007

*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
